                        "name": "until",
                        "time": true,
                        "type": "date",
                        "label": "Until"
                    },
                    {
                        "name": "geo",
                        "spec": [
                            {
                                "name": "latitude",
                                "type": "number",
                                "label": "Latitude",
                                "validate": {
                                    "max": 90,
                                    "min": -90
                                }
                            },
                            {
                                "name": "longitude",
                                "type": "number",
                                "label": "Longitude",
                                "validate": {
                                    "max": 180,
                                    "min": -180
