                            },
                            {
                                "name": "radius",
                                "type": "select",
                                "label": "Radius",
                                "validate": {
                                    "enum": [
                                        "mi",
                                        "km"
                                    ]
                                }
                            }
                        ],
                        "type": "collection",
                        "label": "Geographical Coordinates"
                    },
                    {
                        "name": "include_entities",
                        "type": "boolean",
                        "label": "Include Entities",
                        "required": true
                    }
                ],
                "advanced": true
            }
        },
        {
            "id": 2,
            "module": "util:TextAggregator",
            "version": 1,
            "parameters": {
                "feeder": 1,
                "rowSeparator": "\n"
            },
            "filter": {
                "name": "Remove RTs",
                "conditions": [
                    [
                        {
                            "a": "{{1.text}}",
                            "b": "RT @",
                            "o": "text:notstartwith"
                        }
                    ]
                ]
            },
            "mapper": {
                "value": "{{substring(1.text; 0; 30)}}\nhttps://twitter.com/{{1.user.screen_name}}/status/{{1.id_str}}\n"
            },
            "metadata": {
                "designer": {
                    "x": 300,
                    "y": 0
                },
                "restore": {
                    "feeder": {
                        "label": "Twitter - Search Tweets [1]"
                    },
                    "rowSeparator": {
                        "label": "New row"
                    }
                },
                "parameters": [
                    {
                        "name": "rowSeparator",
                        "type": "select",
                        "label": "Row separator",
                        "validate": {
                            "enum": [
                                "\n",
                                "\t",
                                "other"
                            ]
                        }
                    }
                ],
                "expect": [
                    {
                        "name": "value",
                        "type": "text",
                        "label": "Text",
                        "multiline": true
                    }
                ],
                "advanced": true
            }
        },
