                        "validate": {
                            "enum": [
                                "mixed",
                                "recent",
                                "popular"
                            ]
                        }
                    },
                    {
                        "name": "lang",
                        "type": "text",
                        "label": "Language",
                        "validate": {
                            "max": 2
                        }
                    },
